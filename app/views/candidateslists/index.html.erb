<h1>Candidates</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>CNIC</th>
      <th>Email</th>
      <th>Constituency</th>
      <th>Type</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userlists.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.cnic %></td>
        <td><%= user.email %></td>
        <td><%= user.constituency_name %></td>
        <td><%= user.user_status %></td>
        <td><%= user.total_votes_gain %></td>

        <td>
          <% if current_user.voted == false %>


            <%= button_to "Vote",userlist_path(user),method: :patch%>

          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

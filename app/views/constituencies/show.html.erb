<h1>Candidates</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>CNIC</th>
      <th>Email</th>
      <th>Constituency</th>
      <th>Total votes gain</th>
      <th>Party name</th>
      <th>Symbol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userlists.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.cnic %></td>
        <td><%= user.email %></td>
        <td><%= user.constituency_name %></td>
        <td><%= user.total_votes_gain %></td>
        <td><%= user.party_name%></td>
        <td><%= image_tag user.avatar, width: "100px" if user.avatar.attached?%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h1>Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Profile pic</th>
      <th>Name</th>
      <th>CNIC</th>
      <th>Email</th>
      <th>Constituency</th>
      <th>Type</th
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userlists.each do |user| %>
      <tr>
        <td><%= image_tag user.image, width: "100px" if user.image.attached?%>
        <td><%= user.name %></td>
        <td><%= user.cnic %></td>
        <td><%= user.email %></td>
        <td><%= user.constituency_name %></td>
        <td><%= user.user_status %></td>

      </tr>
    <% end %>
  </tbody>
</table>
